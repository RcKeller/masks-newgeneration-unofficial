{
	"name": "masks-newgeneration-unofficial",
	"version": "2.0.0",
	"author": "Andrew Keller Barrows",
	"description": "Custom Implementation of to the Masks system for Foundry VTT",
	"browserslist": [
		"last 3 versions"
	],
	"license": "MIT",
	"private": true,
	"scripts": {
		"build": "sass src/scss/masks-sheets.scss css/masks-sheets.css --style=expanded --no-source-map",
		"watch": "sass src/scss/masks-sheets.scss css/masks-sheets.css --style=expanded --source-map --watch",
		"uuid": "node ./tools/generate-uuids.mjs",
		"generate:villains": "node ./tools/generate-villains.mjs --indir './resources/villains'",
		"pushLDBtoJSON": "node ./tools/pushLDBtoJSON.mjs",
		"pullJSONtoLDB": "node ./tools/pullJSONtoLDB.mjs"
	},
	"devDependencies": {
		"@anthropic-ai/sdk": "^0.67.0",
		"@eslint/js": "^9.16.0",
		"@foundryvtt/foundryvtt-cli": "^1.0.3",
		"crypto": "^1.0.1",
		"dotenv": "^17.2.3",
		"eslint": "^9.16.0",
		"globals": "^15.13.0",
		"pdf-parse": "^2.4.5",
		"repomix": "^1.9.1",
		"sass": "^1.53.0"
	}
}
